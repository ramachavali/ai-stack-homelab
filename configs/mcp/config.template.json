{
  "mcpServers": {
    "n8n": {
      "type": "streamable-http",
      "url": "http://n8n-mcp:3000/mcp",
      "headers": {
        "Authorization": "Bearer ${N8N_MCP_AUTH_TOKEN}"
      }
    },
    "time": {
      "command": "uvx",
      "args": ["mcp-server-time", "--local-timezone=America/New_York"]
    }
  }
}
